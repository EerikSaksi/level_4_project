{
  "expo": {
    "name": "Stat Buff",
    "slug": "stat-buff",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.statbuff",
    },
    "android": {
      "package": "com.statbuff",
      "versionCode": 13,
      "permissions": []
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "assetBundlePatterns": [
      "assets/cropped_sprites/*"
    ]
  }
}
